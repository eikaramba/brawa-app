    "sass": "^1.42.0",
    "svelte": "~3.42.6",
    "svelte-loader": "~3.1.1",
    "svelte-native-preprocessor": "^1.0.0",
    "svelte-preprocess": "^4.9.5",
    "tailwindcss": "^2.2.15",
    "typescript": "~4.4.3"
