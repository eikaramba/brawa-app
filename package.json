  "dependencies": {
    "@klippa/nativescript-http": "^3.0.1",
    "@nativescript-community/ui-material-core": "^6.0.4",
    "@nativescript-community/ui-svg": "^0.0.8",
    "@nativescript-community/universal-links": "^2.0.3",
    "@nativescript/core": "~8.7.2",
    "@nativescript/firebase-core": "^2.3.4",
